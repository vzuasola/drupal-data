<?php

/**
 * @file
 * Filesource Uninstall the configs.
 */

/**
 * Implements hook_uninstall().
 */
function dafasports_call_us_uninstall() {
  $keys = [
    'dafasports_call_us.call_us_configuration',
  ];

  foreach ($keys as $key) {
    \Drupal::configFactory()->getEditable($key)->delete();
  }
}
