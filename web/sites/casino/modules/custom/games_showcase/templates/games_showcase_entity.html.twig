{#
/**
 * @file games_showcase_entity.html.twig
 * Default theme implementation to present Games Showcase entity data.
 *
 * This template is used when viewing Games Showcase entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_games_showcase_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('games_showcase_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
