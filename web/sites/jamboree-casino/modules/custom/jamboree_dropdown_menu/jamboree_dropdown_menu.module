<?php

/**
 * @file
 * Contains jamboree_dropdown_menu.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function jamboree_dropdown_menu_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the jamboree_dropdown_menu module.
    case 'help.page.jamboree_dropdown_menu':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Main Menu for Jamboree Casino site') . '</p>';
      return $output;

    default:
  }
}
