<?php

/**
 * @file
 * Contains zipang_desktop_block.page.inc.
 *
 * Page callback for Zipang desktop block entities.
 */

use Drupal\Core\Render\Element;

/**
 * Prepares variables for Zipang desktop block templates.
 *
 * Default template: zipang_desktop_block.html.twig.
 *
 * @param array $variables
 *   An associative array containing:
 *   - elements: An associative array containing the user information and any
 *   - attributes: HTML attributes for the containing element.
 */
function template_preprocess_zipang_desktop_block(array &$variables) {
  // Fetch ZipangDesktopBlock Entity Object.
  $zipang_desktop_block = $variables['elements']['#zipang_desktop_block'];

  // Helpful $content variable for templates.
  foreach (Element::children($variables['elements']) as $key) {
    $variables['content'][$key] = $variables['elements'][$key];
  }
}
