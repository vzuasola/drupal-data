<?php

/**
 * @file
 * Contains zipang_mobile_blocks.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function zipang_mobile_blocks_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the zipang_mobile_blocks module.
    case 'help.page.zipang_mobile_blocks':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Zipang Mobile Blocks') . '</p>';
      return $output;

    default:
  }
}
