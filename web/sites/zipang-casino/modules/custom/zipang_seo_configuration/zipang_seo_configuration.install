    'core.entity_form_display.zipang_seo_config_entity.zipang_seo_config_entity.default.yml',
    'core.entity_view_display.paragraph.zipang_metatag.default.yml',
    'core.entity_view_display.zipang_seo_config_entity.zipang_seo_config_entity.default.yml',
    'field.field.paragraph.zipang_metatag.field_content.yml',
    'field.field.paragraph.zipang_metatag.field_metatag_name.yml',
    'field.field.zipang_seo_config_entity.zipang_seo_config_entity.field_meta.yml',
