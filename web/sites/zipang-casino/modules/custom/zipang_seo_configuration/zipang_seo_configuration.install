    'paragraphs.paragraphs_type.zipang_metatag.yml',
    'views.view.zipang_seo_metatag_entity_api.yml',
  ];

  foreach ($keys as $key) {
    \Drupal::configFactory()->getEditable($key)->delete();
  }
}
