{#
/**
 * @file nextbet_front_block.html.twig
 * Default theme implementation to present Nextbet front block data.
 *
 * This template is used when viewing Nextbet front block pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_nextbet_front_block()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('nextbet_front_block') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
