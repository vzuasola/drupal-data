<?php

/**
 * @file
 * Contains gts.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function gts_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the gts module.
    case 'help.page.gts':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Games configuration module') . '</p>';
      return $output;

    default:
  }
}
